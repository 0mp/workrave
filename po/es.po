#: src/core/DistributionSocketLink.cc:640
#: src/core/DistributionSocketLink.cc:665
#, c-format
msgid "Removing client %s:%d."
msgstr "Borrando cliente %s:%d."

#. Closing direct connection.
#: src/core/DistributionSocketLink.cc:702
#, c-format
msgid "Disconnecting %s:%d."
msgstr "Desconectando de %s:%d."

#. It's a remote client. mark it master.
#: src/core/DistributionSocketLink.cc:870
#, c-format
msgid "Client %s:%d is now master."
msgstr "El cliente %s:%d es ahora amo."

#. Its ME!
#: src/core/DistributionSocketLink.cc:876
msgid "I'm now master."
msgstr "Ahora soy yo amo."

#: src/core/DistributionSocketLink.cc:1205
#, c-format
msgid "Client %s:%d saying hello."
msgstr "El cliente %s:%d dice 'Hola'."

#. Duplicate client. inform client that it's bogus and close.
#: src/core/DistributionSocketLink.cc:1220
#: src/core/DistributionSocketLink.cc:1337
#: src/core/DistributionSocketLink.cc:1573
#, c-format
msgid "Client %s:%d is duplicate."
msgstr "El cliente %s:%d está duplicado."

#. Incorrect password.
#: src/core/DistributionSocketLink.cc:1229
#, c-format
msgid "Client %s:%d access denied."
msgstr "Acceso denegado al cliente %s:%d."

#: src/core/DistributionSocketLink.cc:1296
#, c-format
msgid "Client %s:%d signed off."
msgstr "El cliente %s:%d se ha dado de baja."

#: src/core/DistributionSocketLink.cc:1375
#, c-format
msgid "Client %s:%d is welcoming us."
msgstr "El cliente %s:%d le da la bienvenida."

#: src/core/DistributionSocketLink.cc:1608
#, c-format
msgid "Requesting master status from %s:%d."
msgstr "Solicitando el estatuo de amo a %s:%d."

#: src/core/DistributionSocketLink.cc:1621
#, c-format
msgid "Client timeout from %s:%d."
msgstr "Tiempo sobrepasado del cliente %s:%d."

#: src/core/DistributionSocketLink.cc:1642
#, c-format
msgid "Rejecting master request from client %s:%d."
msgstr "Denegada la solicitud del cliente %s:%d de estatuto de amo."

#: src/core/DistributionSocketLink.cc:1648
#, c-format
msgid "Acknowledging master request from client %s:%d."
msgstr "Confirmado estatuto de amo del cliente %s:%d."

#: src/core/DistributionSocketLink.cc:1699
#, c-format
msgid "Non-master client %s:%d rejected master request."
msgstr ""
"El cliente %s:%d sin estatuto de amo ha rechazado la solicitud del amo."

#: src/core/DistributionSocketLink.cc:1704
