<oaf_info>

        <oaf_server     iid="OAFIID:GNOME_WorkraveApplet_Factory"
	                type="exe"
	                location="@LIBEXECDIR@/workrave-applet">

        	<oaf_attribute name="repo_ids" type="stringv">
	        	<item value="IDL:Bonobo/GenericFactory:1.0"/>
		        <item value="IDL:Bonobo/Unknown:1.0"/>
	        </oaf_attribute>
        	<oaf_attribute name="name" type="string" value="Workrave Factory"/>
        	<oaf_attribute name="description" type="string" value="Workrave Applet"/>
        </oaf_server>

        <oaf_server     iid="OAFIID:GNOME_WorkraveApplet"
	                type="factory" 
                        location="OAFIID:GNOME_WorkraveApplet_Factory">

        	<oaf_attribute name="repo_ids" type="stringv">
		        <item value="IDL:GNOME/Vertigo/PanelAppletShell:1.0"/>
		        <item value="IDL:Bonobo/Control:1.0"/>
		        <item value="IDL:Bonobo/Unknown:1.0"/>
	        </oaf_attribute>
	        <oaf_attribute name="name" type="string" _value="Workrave"/>
	        <oaf_attribute name="description" type="string" _value="Workrave Applet"/>
	        <oaf_attribute name="panel:category" type="string" value="Utility"/>
	        <oaf_attribute name="panel:icon" type="string" value="workrave/workrave-icon-huge.png"/>
        </oaf_server>

	<oaf_server     iid="OAFIID:GNOME_Workrave_AppletControl"
	        	type="factory"
		        location="OAFIID:GNOME_WorkraveApplet_Factory">

		<oaf_attribute name="repo_ids" type="stringv">
			<item value="IDL:GNOME/Workrave/AppletControl:1.0"/>
		</oaf_attribute>

		<oaf_attribute name="name" type="string" value="Applet Control"/>
		<oaf_attribute name="description" type="string" value="AppletControl"/>
	</oaf_server>

</oaf_info>
